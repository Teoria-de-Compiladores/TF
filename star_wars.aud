tempo 120;

pattern fanfare {
    C4 q; 
    C4 q; 
    C4 q;
    rest e;
    C4 h; 
    G4 h;
}

pattern descent {
    F4 e; 
    E4 e; 
    D4 e;
    C5 h;
    G4 q;
}

pattern cadence {
    F4 e; 
    E4 e; 
    F4 e;
    D4 h;
}

pattern ending {
    rest q;
    C4 q;
    rest q;
    C4 h;
}

play fanfare;

loop 2 {
    loop 2 {
        play descent;
    }
    play cadence;
}

play ending;
